<h2>Dashboard</h2>

<h3>Items</h3>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Price</th>
      <!-- Add other item attributes as needed -->
    </tr>
  </thead>
  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.id %></td>
        <td><%= number_to_currency(item.price) %></td>
        <!-- Add other item details as needed -->
      </tr>
    <% end %>
  </tbody>
</table>

<h3>Orders</h3>
<table>
  <thead>
    <tr>
      <th>Order ID</th>
      <th>User</th>
      <th>Total</th>
      <!-- Add other order attributes as needed -->
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.id %></td>
        <td><%= order.user.email %></td> <!-- Assuming an order belongs to a user -->
        <!-- Add other order details as needed -->
      </tr>
    <% end %>
  </tbody>
</table>